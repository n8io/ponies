.clearfix
  overflow auto
  zoom 1
  clear both
*:focus
  outline none
  -webkit-focus-ring-color transparent
body
  font-family 'Maven Pro','Helvetica Neue','Helvetica',Helvetica,Arial,sans-serif
  transition all 150ms ease-out
  opacity 0

body.resolved
  opacity 1

.bump-bottom
  margin-bottom 0

span.welcome
  font-size 80%
  text-transform uppercase

img.logo
  height 25px
  width 25px
  top 4px
  left 0
  position absolute

a
  color #222
  text-decoration none
  border-bottom 1px dashed #ddd
  &:visited
    color inherit
  &:hover
  &:active
  &:focus
    border-bottom 1px dotted #555

a.no-underline
  text-decoration none
  border-bottom none
  &:hover
  &:active
  &:focus
    text-decoration none
    border-bottom none

.no-q-logo
  padding-top 30px

md-toolbar
  .title
    a
      color #fff
      text-decoration none
.cursor-pointer
  cursor pointer
.admin-edit
  position absolute
  top -5px
  right -15px
md-radio-button:hover
  background-color rgba(63,81,181,.05)
.mgn-bottom-5
  margin-bottom 5px
.mgn-top-5
  margin-top 5px


.no-q-msg
  .title
  .sub-title
    text-align center

md-grid-tile
  box-shadow 1px 1px 10px rgba(0,0,0,.3)
  border 2px solid #555
  border-radius 5px

md-grid-list md-grid-tile md-grid-tile-header
  background-color #555

.lh-20
  line-height 20px

.pad-left-10
  padding-left 10px

.valign-mid
  vertical-align middle

table.track
  border-left 1px solid #555
  border-right 1px solid #555
  border-bottom 1px solid #555
  border-spacing 0
  padding-top 3px
  &:first-child
    padding-top 0
tr.complete
  td.race
    opacity .6
td.track
  padding 5px
  font-size 1.2em
  font-weight bold
  color #fff
  background-color #555
  &:hover
  &:focus
  &:active
    background-color lighten(#555, 10%)
    outline none !important
  .not-started
    font-size 80%
    padding 3px
    margin-left 10px
    border-radius 3px
    color #3197FF
  .finished
    font-size 80%
    padding 3px
    margin-left 10px
    border-radius 3px
    color #aaa
td.race
  padding 5px
  padding-left 10px
  font-weight bold
  &:hover
  &:focus
  &:active
    background-color darken(#fff, 3%)
    outline none !important
td.wager
  padding 3px 5px 2px 20px
  border-bottom 1px solid rgba(0,0,0,.1)
  opacity 1
  font-size 85%
  &.complete
  &.paid
  &.pending
    font-weight normal
    opacity .6
  &.placed
    background-color rgba(0, 123, 255, .05)
  &.canceled
    display none
  &.win
    background-color rgba(green, .05)
  &.lost
    background-color rgba(red, .05)
    .amount
      display inline
      margin-left 5px
      padding 2px 5px
      color darken(red, 20%)
      border-radius 3px
  &:hover
  &:focus
  &:active
    &.win
      background-color rgba(green, .1)
    &.lost
      background-color rgba(red, .1)
    .wager-timestamp-container
      .wager-timestamp-window
        margin-top -16px
.width-100pct
  width 100%

.text-muted
  opacity 0.6
.float-right
  float right
.float-left
  float left
.cursor-pointer
  cursor pointer
.race-result
  margin-left 10px
  padding 1px 3px
  border 1px solid #000
  border-radius 5px
  font-size 80%
.footer
  text-align center
  margin 10px
.winner
  display inline
  margin-left 5px
  padding 2px 5px
  background-color green
  color #fff
  border-radius 3px
.track
  padding-top 3px
.text-center
  text-align center
.mgn-right-5
  margin-right 5px
.mgn-right-7
  margin-right 7px
.mgn-left-5
  margin-left 5px
.winning-wager-icon
  height 8px
  width 8px
  background green
  margin 6px 5px 0 5px
  border-radius 5px
.position-relative
  position relative
.position-absolute
  position absolute
.y-center
  position absolute
  top 50%
  transform translateY(-50%)
.display-inline
  display inline
.wager-timestamp-container
  max-height 15px
  overflow-y hidden
  margin-top 3px
  position relative
  text-align right
  white-space nowrap
  .wager-timestamp-window
    position relative
    transition all 500ms ease-out
    &:hover
    &:focus
    &:active
      margin-top -16px
a.bookmarklet
  background-color #0070FF
  color #fff
  font-size 90%
  padding 3px 5px
  border-radius 3px
  border-bottom none !important
  white-space nowrap

.bookmark-container
  position fixed
  bottom 5px
  left 0
  background-color #fff
  opacity .3
  margin 0
  padding 5px
  &:hover
  &:focus
  &:active
    opacity 1
.presence-container
  position fixed
  bottom 0
  right 0
  .presence-wrapper
    position relative
    float right
    padding 5px 10px
    background rgba(255, 255, 255, 0.5)
    border-radius 5px
    .presence
      float right
      margin-right 8px
      border-radius 5px
      &:after
        content ''
        top 5px
        right 10px
        height 10px
        width 10px
        border-radius 5px
        position absolute
        background-color #ddd
        transition background-color 500ms ease-out
      &.syncing:after
        background-color green
.i-m-syncing
  position fixed
  bottom 0
  left 0
  right 0
  margin 0
  padding 0
  height 5px
  background-color #ddd
  &.yes
    background-color green
  &.no
    background-color red
.race-mtp
  margin-left 10px
  .minutes
    padding 1px 3px
    border-radius 5px
    font-size 80%
    &.NOT_RUNNING
      background-color darken(blue, 20%)
      border 1px solid darken(blue, 30%)
      color #fff
    &.NOT_SOON
      background-color blue
      border 1px solid darken(blue, 10%)
      color #fff
    &.KINDA_SOON
      background-color green
      border 1px solid darken(green, 10%)
      color #fff
    &.SOON
      background-color yellow
      border 1px solid darken(yellow, 10%)
      color #555
    &.VERY_SOON
      background-color orange
      border 1px solid darken(orange, 10%)
      color #fff
    &.NOW
      background-color red
      border 1px solid darken(red, 10%)
      color #fff
    &.RUNNING
      background-color darken(red, 20%)
      color #fff
      border 1px solid darken(red, 30%)
.gif-tut
  text-align center
  margin 15px 0
  img
    max-width 100%
    border 1px solid #555
    border-radius 5px
    box-shadow 0 0 10px 0 #555

.sync-status
  position fixed
  bottom 0
  left 0
  right 0
  height 7px
  background #ddd
  transition background-color 500ms ease-out
  margin 0
  &.on
    background green

.wager-container
  font-size 95%
  .wager-wrapper
    padding 5px 8px 5px 20px
    border 1px solid transparent
    &:hover
    &:active
    &:focus
      .wager-selections
        border 1px solid #fff
    &.pending
      background-color #fff
      color #555
      .wager-selections
        border 1px solid #555
      &:hover
      &:focus
      &:active
        opacity 1
        background-color #eee
    &.won
      background-color #fff
      color green
      opacity .6
      .wager-selections
        border 1px solid darken(green, 10%)
      &:hover
      &:focus
      &:active
        opacity 1
        background-color green
        color #fff
        .wager-selections
          border 1px solid #fff
    &.lost
      background-color #fff
      color #BD212C
      opacity .6
      .wager-selections
        border 1px solid #BD212C
      &:hover
      &:focus
      &:active
        opacity 1
        background-color #BD212C
        color #fff
        .wager-selections
          border 1px solid #fff
    &.canceled
      background-color rgba(gray, .3)
      opacity .6
      text-decoration line-through
      &:hover
      &:focus
      &:active
        opacity 1
    .wager-timestamp-container
      margin-top 0
      max-height 20px
    .wager-selections
      border 1px solid transparent
      font-size 80%
      border-radius 3px
      padding 2px 3px
  &:hover
  &:focus
  &:active
    .wager-timestamp-window
      margin-top -18px

.mtp-container
  padding 1px 3px 1px 5px
  border-radius 5px
  font-size 65%
  float left
  margin-top 4px
  .mtp-wrapper
    padding 2px 3px
    border-radius 3px
    border 1px solid transparent
    font-weight bold
    text-shadow 1px 2px 3px rgba(0,0,0,.2)
    transition all 500ms ease
    &.closed
      color #555
      background-color #F3F3F3
      border 1px solid #D2D2D2
    &.not-running
      background-color #0905A7
      border 1px solid darken(#0905A7, 10%)
      color rgba(#fff, .8)
    &.not-soon
      background-color #2824CA
      border 1px solid darken(#2824CA, 10%)
      color #fff
    &.kinda-soon
      background-color #0C99BD
      border 1px solid darken(#0C99BD, 10%)
      color #fff
    &.fairly-soon
      background-color #7FC147
      border 1px solid darken(#7FC147, 10%)
      color #fff
    &.soon
      background-color yellow
      border 1px solid darken(yellow, 30%)
      text-shadow 1px 2px 3px rgba(0,0,0,.08)
      color #555
    &.very-soon
      background-color #FF6C00
      border 1px solid darken(#FF6C00, 20%)
      color #fff
    &.now
      background-color red
      border 1px solid darken(red, 10%)
      color #fff
    &.off
      background-color darken(red, 25%)
      color #fff
      border 1px solid darken(red, 35%)

.track-outer
  margin-bottom 5px
  border 1px solid #555
  border-radius 3px 3px 0 0
  &.finished
    opacity .8
  &:hover
  &:active
  &:focus
    opacity 1
  .track-container
    font-size 115%
    background-color #fff
    border-bottom 1px solid #555
    transition border 200ms ease
    border-radius 3px 3px 0 0
    .track-name
      padding 5px 10px
      color #fff
      background-color #555
      max-width 250px
      width 250px
      text-overflow ellipsis
      white-space nowrap
      overflow-x hidden
    .track-wrapper
      .track-toggle
        margin 4px 5px 0 0
td.race-cell
  &.final
    opacity .7
    &:hover
    &:active
    &:focus
      opacity 1
    * .wager-wrapper.won
      opacity 1
      font-weight bold
      background-color rgba(127, 210, 101, 0.44)
      &:hover
      &:focus
      &:active
        background-color green

.race-container
  &:hover
    background-color #f9f9f9
  .race-wrapper
    padding 5px 5px 5px 15px
    border-bottom 1px solid #ddd
    .race-name
      font-size 105%
.wps-container
  padding 2px 5px 3px
  font-size 80%
  background #eee
  margin-left 10px
  border 1px solid #ddd
  text-shadow 0 1px 0 #ccc
  border-radius 3px
  color #555
  &:hover
    text-decoration underline
.wager-payout
  font-size 90%
  &.winner
    display inline
    margin-left 0px
    padding 2px 5px
    background-color green
    color #fff
    border-radius 3px
.wwd-container
  float left
  &.shown
    opacity 1
    .wwd-wrapper
      .wwd-dot
        border 1px solid transparent
        background-color green
  .wwd-wrapper
    .wwd-dot
      transition all 500ms ease
      border 1px solid green
      float left
      background-color transparent
      height 8px
      width 8px
      border-radius 5px
      margin 6px 0 0 5px
.wps-detail-exotic-combo
  margin-right -5px
  padding 0
.wps-detail-exotic-win-amount
  text-align right
.wps-details-wrapper
  max-width 700px
  margin 0 auto
  > *
    margin inherit 0
    padding 0
  h4
    font-size 1.2em
  table
    border-radius 3px
    border 1px solid #555
    margin 10px 0
    padding 10px
    border-spacing 0px
    border-collapse separate
    font-size 90%
    th.wps-header
      &.horse
        text-align center
        min-width 40px
      &.odds
        text-align right
        min-width 30px
      &.ml
        opacity .7
        text-align right
        min-width 30px
        padding-left 5px
        font-weight normal

      &.odds
      &.ml
        &:hover
          text-decoration underline
      &.win
      &.place
      &.show
        text-align right
    th.exotic-header
      &.payout
        text-align right
      &.type
      &.base
      &.combo
        text-align left
    tr:nth-child(even)
      background-color #ddd

  .wps-detail-horse
    text-align center
  .wps-detail-win
  .wps-detail-place
  .wps-detail-show
    text-align right
.horse-number-container
    text-align center
    padding 3px 0
    border-radius 3px
    max-width 50px
    margin 0 auto
    font-family monospace
.horse-odds-actual
  text-align right
  font-weight bold
  margin-right 5px
.horse-odds-morning-line
  text-align right
  opacity .7